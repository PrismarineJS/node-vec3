{
  "name": "vec3",
  "version": "0.1.10",
  "description": "3d vector math with good unit tests",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "npm run test-js && npm run test-types",
    "test-js": "mocha --reporter spec",
    "test-types": "tsd",
    "pretest": "npm run lint",
    "lint": "standard",
    "fix": "standard --fix"
  },
  "keywords": [
    "point"
  ],
  "exports": {
    "types": "./index.d.ts",
    "require": "./index.js",
    "import": "./wrapper.mjs"
  },
  "author": "Andrew Kelley",
  "license": "BSD",
  "devDependencies": {
    "mocha": "^10.0.0",
    "standard": "^17.0.0",
    "tsd": "^0.25.0"
  },
  "dependencies": {},
  "tsd": {
    "directory": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PrismarineJS/node-vec3.git"
  }
}
